{
  "manifest_version": 2,
  "name": "Blocksmurfer Blockchain Explorer",
  "version": "1",
  "short_name": "Blocksmurfer",
  "default_locale": "en",
  "description": "Search the bitcoin blockchain for addresses, transactions, blocks and keys. Includes all the nerdy details!",
  "author": "Coineva",
  "icons": [
    {
      "src": "/static/favicon.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/static/favicon.ico",
      "sizes": "16x16",
      "type": "image/x-icon"
    },
    {
      "src": "/static/favicon-16x16.png",
      "sizes": "16x16",
      "type": "image/png"
    },
    {
      "src": "/static/favicon-32x32.png",
      "sizes": "32x32",
      "type": "image/png"
    },
    {
      "src": "/static/apple-touch-icon.png",
      "sizes": "180x180",
      "type": "image/png"
    },
    {
      "src": "/static/android-chrome-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "theme_color": "#ffffff",
  "background_color": "#ffffff",
  "display": "standalone"
}